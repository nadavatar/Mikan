{
  "extends": [
    "tslint-config-standard",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-boolean-literal-compare": true,
    "import-name": false,
    "function-name": false,
    "no-else-after-return": false,
    "no-unused-variable": {
      "severity": "warn"
    },
    "variable-name": false,
    "prefer-array-literal": false
  }
}